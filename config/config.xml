<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" id="http://www.obigo.com/ces2020/OBIGO/C300/store" version="1.0.7" viewmodes="fullscreen">
  <icon src="icon.png"/>
  <author>Obigo</author>
  <license>        THE SOFTWARE IS PROVIDED BY OBIGO.
    </license>
  <content src="index.html" type="text/html"/>
  <description xml:lang="en-us">App center for all apps in Home. You can browse new or updated apps available for your car and install them as you need. You can browse the whole range of downloaded apps in your car.</description>
  <name xml:lang="en-us" short="Store">Store</name>
  <access origin="*"/>
  <gadgets>
    <gadget size="s" name="small"/>
  </gadgets>
  <feature name="http://www.obigo.com/api/applicationmanager.getownerapplication" required="true">
    <param name="version" value="3.1.5.6"/>
  </feature>
  <feature name="http://www.obigo.com/api/applicationmanager.widgets" required="true">
    <param name="version" value="3.1.5.6"/>
  </feature>
  <feature name="http://www.obigo.com/api/applicationmanager.getrunningapplications" required="true">
    <param name="version" value="3.1.5.6"/>
  </feature>
  <feature name="http://www.obigo.com/api/applicationmanager.installwidget" required="true">
    <param name="version" value="3.1.5.6"/>
  </feature>
  <feature name="http://www.obigo.com/api/applicationmanager.uninstallwidget" required="true">
    <param name="version" value="3.1.5.6"/>
  </feature>
  <feature name="http://www.obigo.com/api/applicationmanager.checklatestapplications" required="true">
    <param name="version" value="3.1.5.6"/>
  </feature>
  <feature name="http://www.obigo.com/api/storage" required="true">
    <param name="version" value="1.1.0.0"/>
  </feature>
  <feature name="http://www.obigo.com/api/network" required="true">
    <param name="version" value="1.1.0.0"/>
  </feature>
  <feature name="http://www.obigo.com/api/downloadmanager" required="true">
    <param name="version" value="3.1.5.6"/>
  </feature>
  <meta-data>
    <manifest-version>1.3.2.1</manifest-version>
    <store-server>
      <address>cspappcenter.obigo.com</address>
      <path>csp</path>
      <port>443</port>
    </store-server>
  </meta-data>
</widget>